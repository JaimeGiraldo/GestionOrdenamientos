  <connectionStrings>
    <add name="CnxSqlServer" connectionString="Data Source=192.168.2.16;Initial Catalog=dbEvolutionAdvanced;Persist Security Info=True;User ID=PromedanNet;pwd=PromedanNet2013" />
    <add name="dbEvolutionAdvancedConnectionString" connectionString="Data Source=192.168.2.16;Initial Catalog=dbEvolutionAdvanced;Persist Security Info=True;User ID=PromedanNet;Password=PromedanNet2013"
      providerName="System.Data.SqlClient" />
